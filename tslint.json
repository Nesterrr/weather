{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-hooks"
  ],
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "interface-name": false,
    "quotemark": [true, "single", "avoid-escape", "jsx-double"],
    "react-hooks-nesting": "error"
  }
}
